{
    "name": "mathias-lund/zenfit",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "AdminBundle\\": "src/AdminBundle",
            "ApiBundle\\": "src/ApiBundle",
            "AppBundle\\": "src/AppBundle",
            "ChatBundle\\": "src/ChatBundle",
            "ClientBundle\\": "src/ClientBundle",
            "EmailBundle\\": "src/EmailBundle",
            "LenusBundle\\": "src/LenusBundle",
            "ExerciseBundle\\": "src/ExerciseBundle",
            "GymBundle\\": "src/GymBundle",
            "LeadBundle\\": "src/LeadBundle",
            "MealBundle\\": "src/MealBundle",
            "PlanBundle\\": "src/PlanBundle",
            "ProgressBundle\\": "src/ProgressBundle",
            "ReactApiBundle\\": "src/ReactApiBundle",
            "TrainerBundle\\": "src/TrainerBundle",
            "VideoBundle\\": "src/VideoBundle",
            "WorkoutPlanBundle\\": "src/WorkoutPlanBundle",
            "ZapierBundle\\": "src/ZapierBundle",
            "Zenfit\\": "src/Zenfit"
        },
        "classmap": [
            "src/Kernel.php",
            "src/EntityIdTrait.php"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-ctype": "*",
        "ext-mbstring": "*",
        "ext-pdo_mysql": "*",
        "ext-intl": "*",
        "ext-xml": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "gesdinet/jwt-refresh-token-bundle": "^1.0.0",
        "lexik/jwt-authentication-bundle": "^2.8",
        "doctrine/orm": "^2.7",
        "doctrine/doctrine-bundle": "^2.1",
        "friendsofsymfony/user-bundle": "dev-php8",
        "emarref/jwt": "^1.0",
        "aws/aws-sdk-php": "^3.11",
        "aws/aws-sdk-php-symfony": "^2.2",
        "stripe/stripe-php": "^7.0",
        "myclabs/deep-copy": "^1.5",
        "suncat/mobile-detect-bundle": "dev-master",
        "intervention/image": "^2.3",
        "knplabs/knp-time-bundle": "^v1.14",
        "danielstjules/stringy": "^3.0",
        "league/fractal": "^0.19",
        "google/apiclient": "^2.1",
        "asimlqt/php-google-spreadsheet-client": "^3.0",
        "ivopetkov/video-embed": "^v0.2",
        "jimmiw/php-time-ago": "^2.0",
        "illuminate/support": "^v7.30",
        "pusher/pusher-php-server": "^4.1",
        "elao/enum": "^1.5",
        "php-http/httplug-bundle": "^1.19",
        "php-http/curl-client": "^2.2",
        "sendgrid/sendgrid": "^7.9",
        "stof/doctrine-extensions-bundle": "^1.6",
        "beberlei/doctrineextensions": "^v1.3",
        "gedmo/doctrine-extensions": "^v3.2",
        "nelmio/cors-bundle": "^2.1",
        "symfony/console": "^5.2",
        "symfony/flex": "^1.3",
        "symfony/framework-bundle": "^5.2",
        "symfony/asset": "^5.2",
        "symfony/serializer": "^5.2",
        "symfony/dom-crawler": "^5.2",
        "symfony/process": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/intl": "^5.2",
        "doctrine/annotations": "^1.11",
        "symfony/http-client": "^5.2",
        "symfony/mailer": "^5.2",
        "symfony/messenger": "^5.2",
        "symfony/amazon-sqs-messenger": "^5.2",
        "symfony/proxy-manager-bridge": "^5.2",
        "symfony/doctrine-messenger": "^5.2",
        "symfony/mime": "^5.2",
        "symfony/translation": "^5.2",
        "symfony/twig-bundle": "*",
        "twig/twig": "^2.14",
        "twig/extra-bundle": "^2.14",
        "symfony/yaml": "^5.2",
        "symfony/string": "^5.2",
        "twig/cssinliner-extra": "^v2.14",
        "twig/inky-extra": "^v2.14",
        "symfony/sendgrid-mailer": "^v5.2",
        "ocramius/doctrine-batch-utils": "^2.2",
        "egulias/email-validator": "^2.1",
        "symfony/monolog-bundle": "^3.5",
        "sensio/framework-extra-bundle": "^5.5",
        "incenteev/composer-parameter-handler": "^2.1",
        "norkunas/onesignal-php-api": "^v2.3",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "doctrine/cache": "^1.10",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "drewm/mailchimp-api": "^2.5",
        "ext-imagick": "*",
        "sentry/sentry-symfony": "^4.0",
        "nyholm/psr7": "^1.3",
        "symfony/lock": "^5.2",
        "spatie/pdf-to-image": "^2.1",
        "mixpanel/mixpanel-php": "^2.8"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "@stable",
        "phpstan/phpdoc-parser": "@stable",
        "phpstan/phpstan": "@stable",
        "phpstan/phpstan-deprecation-rules": "@stable",
        "phpstan/phpstan-strict-rules": "@stable",
        "phpstan/phpstan-doctrine": "@stable",
        "phpstan/phpstan-phpunit": "@stable",
        "phpstan/phpstan-symfony": "@stable",
        "phpstan/phpstan-webmozart-assert": "@stable",
        "psalm/plugin-phpunit": "@stable",
        "psalm/plugin-symfony": "@stable",
        "vimeo/psalm": "@stable",
        "weirdan/doctrine-psalm-plugin": "@stable",
        "symfony/maker-bundle": "@stable",
        "phpstan/extension-installer": "@stable",
        "phpunit/phpunit": "@stable",
        "symfony/browser-kit": "@stable",
        "symfony/css-selector": "@stable",
        "symfony/phpunit-bridge": "@stable",
        "symfony/stopwatch": "@stable",
        "symfony/web-profiler-bundle": "@stable"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Zenfit-ApS/FOSUserBundle.git"
        }
    ],
    "scripts": {
        "build-params": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative web": "symfony-cmd"
        },
        "post-update-cmd": [
            "@build-params",
            "@auto-scripts"
        ],
        "post-install-cmd": [
            "@build-params",
            "@auto-scripts"
        ]
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "require": "5.3.*",
            "allow-contrib": true
        },
        "incenteev-parameters": {
            "file": "config/parameters.yaml",
            "env-map": {
                "stripe_publishable_key": "ZF_STRIPE_PUBLISHABLE_KEY",
                "stripe_secret_key": "ZF_STRIPE_SECRET_KEY",
                "sendgrid_api_key": "SG_API_KEY",
                "youtubeApiKey": "YOUTUBE_API_KEY",
                "assets_version": "ASSETS_VERSION",
                "mailer_user": "MAILER_USER",
                "mailer_password": "MAILER_PASSWORD",
                "jwt_secret": "JWT_SECRET",
                "memcached_servers": "MEMCACHED_URL",
                "myfitnesspal.client_id": "MYFITNESSPAL_CLIENT_ID",
                "myfitnesspal.client_secret": "MYFITNESSPAL_CLIENT_SECRET"
            }
        }
    }
}
